{"ast":null,"code":"import { Navigation, Pagination, Scrollbar, A11y } from 'swiper/modules';\nimport { Swiper, SwiperSlide } from 'swiper/vue';\nimport { ref } from 'vue';\nimport CustomButton from \"@/components/CustomButton.vue\";\nimport BurgerMenu from \"@/components/BurgerMenu.vue\";\nexport default {\n  components: {\n    CustomButton,\n    Swiper,\n    SwiperSlide,\n    BurgerMenu\n  },\n  setup() {\n    const slides = ref(['Bitmap.png', 'Bitmap.png', 'Bitmap.png']);\n    const getBackgroundStyle = slide => {\n      const imageUrl = require(`../assets/${slide}`);\n      return {\n        backgroundImage: `url(${imageUrl})`,\n        backgroundSize: 'cover',\n        backgroundPosition: 'center',\n        width: '100%',\n        height: '900px'\n      };\n    };\n    return {\n      slides,\n      getBackgroundStyle,\n      modules: [Navigation, Pagination, Scrollbar, A11y]\n    };\n  }\n};","map":{"version":3,"names":["Navigation","Pagination","Scrollbar","A11y","Swiper","SwiperSlide","ref","CustomButton","BurgerMenu","components","setup","slides","getBackgroundStyle","slide","imageUrl","require","backgroundImage","backgroundSize","backgroundPosition","width","height","modules"],"sources":["/Users/maksymsenyuk/Desktop/projects/test/src/components/TopSlider.vue"],"sourcesContent":["<template>\n  <div class=\"swiper-container\">\n    <swiper\n        :modules=\"modules\"\n        :slides-per-view=\"1\"\n        :space-between=\"0\"\n        :pagination=\"{ clickable: true }\"\n    >\n      <swiper-slide v-for=\"slide in slides\" :key=\"slide\">\n        <div :style=\"getBackgroundStyle(slide)\" class=\"swiper-container__slide\">\n          <div class=\"container\">\n            <div class=\"swiper-container__header\">\n              <div class=\"swiper-container__header__social-media\">\n                <a href=\"#\"><font-awesome-icon icon=\"fa-brands fa-instagram\"/></a>\n                <a href=\"#\"><font-awesome-icon icon=\"fa-brands fa-twitter\"/></a>\n                <a href=\"#\"><font-awesome-icon icon=\"fa-brands fa-facebook-f\"/></a>\n              </div>\n              <div class=\"swiper-container__header__navigation\">\n                <router-link to=\"{}\">SHOP</router-link>\n                <router-link to=\"{}\">PLAN MY KITCHEN</router-link>\n              </div>\n              <div class=\"swiper-container__header__logo\">\n                <img alt=\"logo\" src=\"../assets/logo.png\"/>\n              </div>\n              <div class=\"swiper-container__header__navigation\">\n                <router-link to=\"{}\">ABOUT US</router-link>\n                <router-link to=\"{}\">GALLERY</router-link>\n              </div>\n              <div class=\"swiper-container__header__basket\">\n                <button>MY ORDER <font-awesome-icon icon=\"cart-shopping\" /></button>\n              </div>\n            </div>\n            <div class=\"mobile-menu\"  >\n              <burger-menu>\n                <router-link to=\"/\">Home</router-link>\n                <router-link to=\"/about\">About Us</router-link>\n                <router-link to=\"/gallery\">Gallery</router-link>\n                <div class=\"swiper-container__header__mobile-basket\">\n                  <button>MY ORDER <font-awesome-icon icon=\"cart-shopping\" /></button>\n                </div>\n              </burger-menu>\n              <div class=\"swiper-container__mobile__header__logo\">\n                <img alt=\"logo\" src=\"../assets/logo.png\"/>\n              </div>\n            </div>\n\n            <div class=\"swiper-container__divider\"></div>\n            <div class=\"swiper-container__info\">\n              <div class=\"swiper-container__info__subtitle\">\n                <p>Design and order your new kitchen online today</p>\n              </div>\n              <div class=\"swiper-container__info__title\">\n                <h1>Bespoke & made to measure <br/> handmade kitchen design</h1>\n              </div>\n              <div class=\"swiper-container__info__order-now\">\n                <custom-button text=\"Order Now\"></custom-button>\n              </div>\n            </div>\n          </div>\n        </div>\n      </swiper-slide>\n    </swiper>\n\n  </div>\n</template>\n\n<script>\nimport { Navigation, Pagination, Scrollbar, A11y } from 'swiper/modules';\nimport { Swiper, SwiperSlide } from 'swiper/vue';\nimport { ref } from 'vue';\nimport CustomButton from \"@/components/CustomButton.vue\";\nimport BurgerMenu from \"@/components/BurgerMenu.vue\";\nexport default {\n  components: {\n    CustomButton,\n    Swiper,\n    SwiperSlide,\n    BurgerMenu\n  },\n  setup() {\n    const slides = ref(['Bitmap.png', 'Bitmap.png', 'Bitmap.png']);\n\n    const getBackgroundStyle = (slide) => {\n      const imageUrl = require(`../assets/${slide}`);\n      return {\n        backgroundImage: `url(${imageUrl})`,\n        backgroundSize: 'cover',\n        backgroundPosition: 'center',\n        width: '100%',\n        height: '900px',\n      };\n    };\n\n    return {\n      slides,\n      getBackgroundStyle,\n      modules: [Navigation, Pagination, Scrollbar, A11y],\n    };\n  },\n};\n</script>\n\n<style lang=\"less\">\n.swiper-container {\n  width: 100%;\n  max-height: 900px;\n  box-sizing: border-box;\n  @media (max-width: 768px) {\n    max-height: auto;\n  }\n  .swiper-container__slide {\n    background-size: cover;\n    background-position: center;\n    width: 100%;\n    height: 900px;\n  }\n\n  .container {\n\n\n    .swiper-container__header {\n      display: flex;\n      justify-content: space-between;\n      align-content: center;\n      padding-top: 30px;\n      height: 50px;\n\n      > * {\n        padding: 5px;\n        text-decoration: none;\n        color: white;\n        cursor: pointer;\n        display: flex;\n        justify-content: space-between;\n        align-content: center;\n      }\n      .swiper-container__header__social-media {\n        display: flex;\n        padding-left: 13px;\n        > * {\n          padding: 5px;\n          text-decoration: none;\n          color: white;\n          cursor: pointer;\n        }\n      }\n\n      .swiper-container__header__navigation {\n        > * {\n          font-family: Roboto-Regular;\n          padding: 5px;\n          height: 13px;\n          text-decoration: none;\n          cursor: pointer;\n          letter-spacing: 3px;\n          text-align: center;\n          color: #FFFFFF;\n          text-transform: uppercase;\n          opacity: 1;\n\n          &:nth-child(2) {\n            margin-left: 20px;\n          }\n        }\n      }\n      .swiper-container__header__basket {\n        button {\n          width: 192px;\n          height: 50px;\n          font-family: Roboto-Regular;\n          border: 1px solid #FFFFFF;\n          border-radius: 25px;\n          opacity: 1;\n          background: none;\n          color: #FFFFFF;\n          text-align: center;\n          letter-spacing: 3px;\n          text-transform: uppercase;\n          margin-top: -10px;\n          cursor: pointer;\n          &:hover {\n            background: #FFFFFF;\n            color: #000000;\n          }\n        }\n      }\n      .swiper-container__header__logo {\n        img {\n          width: 83px;\n          height: 32px;\n        }\n      }\n\n    }\n\n    .swiper-container__header__mobile-basket{\n      button {\n        width: 192px;\n        height: 50px;\n        font-family: Roboto-Regular;\n        border: 1px solid #FFFFFF;\n        border-radius: 25px;\n        opacity: 1;\n        background: none;\n        color: #FFFFFF;\n        text-align: center;\n        letter-spacing: 3px;\n        text-transform: uppercase;\n        margin-top: 10px;\n        cursor: pointer;\n        &:hover {\n          background: #FFFFFF;\n          color: #000000;\n        }\n      }\n    }\n    .mobile-menu {\n      display: none;\n      padding: 20px;\n      color: white;\n    }\n    .swiper-container__mobile__header__logo{\n      margin-top: -20px;\n      img{\n        height: 30px;\n      }\n    }\n\n    @media (max-width: 768px) {\n      .swiper-container__header, .swiper-container__divider {\n        display: none;\n      }\n      .mobile-menu {\n        display: block;\n      }\n\n    }\n    .swiper-container__divider {\n      width: 100%;\n      margin-top: 31px;\n      height: 1px;\n      background: rgba(255, 255, 255, 0.3);\n      opacity: 0.3;\n    }\n\n    .swiper-container__info {\n      padding-top: 244px;\n      display: flex;\n      justify-content: center;\n      flex-direction: column;\n      align-items: center;\n      .swiper-container__info__subtitle {\n        text-align: center;\n        letter-spacing: 3px;\n        color: #FFFFFF;\n        text-shadow: 0px 0px 4px #000000;\n        text-transform: uppercase;\n        opacity: 1;\n        font-family: Roboto-Regular;\n      }\n\n      .swiper-container__info__title {\n        text-align: center;\n        letter-spacing: -1px;\n        color: #FFFFFF;\n        text-shadow: 0px 0px 25px #00000080;\n        opacity: 1;\n        font-family: Helvetica-Bold;\n        h1 {\n          margin: 0;\n        }\n      }\n\n      .swiper-container__info__order-now {\n        margin-top: 30px;\n      }\n    }\n  }\n\n}\n\n\n</style>\n"],"mappings":"AAmEA,SAASA,UAAU,EAAEC,UAAU,EAAEC,SAAS,EAAEC,IAAG,QAAS,gBAAgB;AACxE,SAASC,MAAM,EAAEC,WAAU,QAAS,YAAY;AAChD,SAASC,GAAE,QAAS,KAAK;AACzB,OAAOC,YAAW,MAAO,+BAA+B;AACxD,OAAOC,UAAS,MAAO,6BAA6B;AACpD,eAAe;EACbC,UAAU,EAAE;IACVF,YAAY;IACZH,MAAM;IACNC,WAAW;IACXG;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIL,GAAG,CAAC,CAAC,YAAY,EAAE,YAAY,EAAE,YAAY,CAAC,CAAC;IAE9D,MAAMM,kBAAiB,GAAKC,KAAK,IAAK;MACpC,MAAMC,QAAO,GAAIC,OAAO,CAAE,aAAYF,KAAM,EAAC,CAAC;MAC9C,OAAO;QACLG,eAAe,EAAG,OAAMF,QAAS,GAAE;QACnCG,cAAc,EAAE,OAAO;QACvBC,kBAAkB,EAAE,QAAQ;QAC5BC,KAAK,EAAE,MAAM;QACbC,MAAM,EAAE;MACV,CAAC;IACH,CAAC;IAED,OAAO;MACLT,MAAM;MACNC,kBAAkB;MAClBS,OAAO,EAAE,CAACrB,UAAU,EAAEC,UAAU,EAAEC,SAAS,EAAEC,IAAI;IACn